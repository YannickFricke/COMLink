{
  "compilerOptions": { 
    "target": "es5",
    "module": "commonjs",
    "composite": true,
    "declaration": true, 
    "outDir": "./dist/",
    "rootDir": "./src/", 
    "strict": true, 
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
